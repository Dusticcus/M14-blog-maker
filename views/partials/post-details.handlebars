{{#each posts as |post| }}
<div class="row postWrapper" id="{{post.id}}">
  <div class="postTitle col-md-6">
    <h2>{{post.title}}</h2>

  </div>
  <div class="postName col-md-6">
    <button class="btn">Save Edits</button>
    <p>Posted by {{post.user.name}} at {{post.createdAt}}</p>
  </div>
  <div class="row postContent">
    <p class="changeContent" contentEditable="true">{{post.content}}</p>
  </div>

</div>

{{/each}}