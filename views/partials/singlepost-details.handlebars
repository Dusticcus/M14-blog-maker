<div class="row postWrapper">
  <div class="postTitle col-md-6" id="postId" data-id="{{post.id}}">
    <h2>{{post.title}}</h2>
  </div>
    <div class="postName col-md-6">
    <p>Posted by {{post.user.name}} at {{post.createdAt}}</p>
  </div>
  <div class="row postContent">
    {{post.content}}
  </div>
</div>
